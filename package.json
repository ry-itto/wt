{
  "name": "@ry-itto/wt",
  "version": "1.0.0",
  "description": "Git worktree operations wrapper with interactive interface",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "wt": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest tests/unit",
    "test:e2e": "npm run build && jest tests/e2e",
    "test:watch": "jest --watch tests/unit",
    "test:coverage": "jest --coverage tests/unit",
    "prepare": "npm run build"
  },
  "keywords": ["git", "worktree", "cli", "fzf", "ghq"],
  "author": "Ryoya Ito",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ry-itto/wt.git"
  },
  "bugs": {
    "url": "https://github.com/ry-itto/wt/issues"
  },
  "homepage": "https://github.com/ry-itto/wt#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/jest": "^29.5.8",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^9.29.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}